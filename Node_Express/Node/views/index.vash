@html.extend('layout',function(model){
	@html.block('body',function(model){
	
	<h1>@model.title</h1>
	<p> this is from Body </p>

	@if (model.error)
	{
	<p style='color:red'> Error is : @model.error</p>	 
	}

	@model.categories.forEach(function(item){
	<div>
	<span>@(item.notes? item.notes.length : 0) -</span>
	<span>@item.name</span>
	</div>
	})



	})
})